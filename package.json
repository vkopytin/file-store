{
  "name": "file-store",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "postinstall": "prisma generate --no-engine",
    "generate": "prisma generate",
    "prisma-init": "prisma init",
    "db-pull": "prisma db pull",
    "migrate": "npx prisma migrate diff --from-empty --to-schema prisma/schema.prisma --script > prisma/migrations/0_init/migration.sql",
    "migrate-resolve": "npx prisma migrate resolve --applied 0_init",
    "studio": "npx prisma studio --config ./prisma.config.ts"
  },
  "dependencies": {
    "@prisma/adapter-pg": "^7.1.0",
    "@prisma/client": "^7.1.0",
    "@prisma/extension-accelerate": "^3.0.1",
    "dotenv": "^17.2.3",
    "next": "16.0.5",
    "pg": "^8.16.3",
    "react": "19.2.0",
    "react-dom": "19.2.0"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20.19.25",
    "@types/pg": "^8.15.6",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "16.0.5",
    "prisma": "^7.1.0",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
}
